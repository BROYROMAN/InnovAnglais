{% extends 'base.html.twig' %}
{% block title %}{{ parent()}} Ajout d'un abonnements {% endblock %}
{% block contenu %}
{{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="form-group">
        
                {{ form_label(form.libelle, 'Libelle:', {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        <div class="col-sm-10">
          {{ form_errors(form.libelle) }}
          {{ form_widget(form.libelle) }}
          {{ form_help(form.libelle) }}
        </div>  
    </div>
        
            <div class="form-group">
        
                {{ form_label(form.paiment, 'Paiement:', {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        <div class="col-sm-10">
          {{ form_errors(form.paiment) }}
          {{ form_widget(form.paiment) }}
          {{ form_help(form.paiment) }}
        </div>  
    </div>
    
     
{{ form_end(form) }}
{% endblock %}