{% extends 'base.html.twig' %}
{% block title %}{{ parent()}} Liste des theme {% endblock %}
{% block contenu %}
    <h1> Liste des theme </h1>
    {% if not liste is empty %}
        {{ form_start(form)}} 
    <table class="table">
    <tr><th>libelle</th><th>JSON</th><th>Modifier</th><th>Supprimer</th></tr> 
    {% for key,value in liste %}
<tr><td>{{value.libelle}}</td><td><a href="{{ path('wsTheme',{'id':value.id})}}" >JSON</a><td>  <div  class="btn-group  btn-group-lg"><a href="{{ path('theme_modifier',{'id':value.id})}}"><span
class="glyphicon  glyphicon-pencil"  aria-hidden="true"></span></a></div></td><td><input type="checkbox" name="cocher[]" value="{{ value.id }}"/></td></tr> 
    {% endfor %}
    </table>
    {{ form_end(form)}}
{% else %}
    Aucun theme Ã  afficher
{% endif %}
{% endblock %}