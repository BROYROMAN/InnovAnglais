{% extends 'base.html.twig' %}
{% block title %}{{ parent()}} Ajout d'une enreprise {% endblock %}
{% block contenu %}
    <div class="container">
    {{ form_start(form) }}
    {{ form_errors(form) }}
     <div class="form-group">
    {{  form_label(form.libelle,  'Libelle:',  {'label_attr':  {'class':  'col-sm-2  control-label'}}) }} 
        <div class="col-sm-10">
        {{ form_errors(form.libelle) }} 
        {{ form_widget(form.libelle) }} 
        {{ form_help(form.libelle) }}
        </div>
    </div>  
    
    
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
        {{ form_widget(form.save) }} 
        </div>
    </div>
    {{ form_end(form) }}  
    </div>
{% endblock %}   