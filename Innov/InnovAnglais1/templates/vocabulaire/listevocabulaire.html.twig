{% extends 'base.html.twig' %}
{% block title %}{{ parent()}} Liste des vocabulaire {% endblock %}
{% block contenu %}
    <h1> Liste des vocabulaire </h1>
    {% if not liste is empty %}
        {{ form_start(form)}} 
    <table class="table">
    <tr><th>libelle</th><th>Categories</th><th>Nom du fichier </th><th>Date</th><th>Extension</th><th>Taille</th><th>Nom Original</th><th>Photo</th><th>JSON</th><th>Modifier</th><th>Supprimer</th></tr> 
    {% for key,value in liste %}
        <tr><td>{{value.libelle}}</td><td>{{value.categories}}</td><td>{{ value.nom }}</td><td>{{ value.date|date("d/m/Y") }}</td><td>{{ value.extension}}</td><td>{{ value.taille}}</td><td>{{ value.nomoriginal}}</td><td><img src="uploads/fichiers/{{value.nom}}" WIDTH=150" HEIGHT=70 alt="" /></td><td><a href="{{ path('wsVocabulaire3',{'id':value.user.id})}}" >JSON</a></td><td>  <div  class="btn-group  btn-group-lg"><a href="{{ path('vocabulaire_modifier',{'id':value.id})}}"><span
class="glyphicon  glyphicon-pencil"  aria-hidden="true"></span></a></div></td><td><input type="checkbox" name="cocher[]" value="{{ value.id }}"/></td></tr> 
    {% endfor %}
    </table>
    {{ form_end(form)}}
{% else %}
    Aucun vocabulaire Ã  afficher
{% endif %}
{% endblock %}